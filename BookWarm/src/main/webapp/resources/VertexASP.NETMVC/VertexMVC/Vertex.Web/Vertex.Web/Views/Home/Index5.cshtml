                            <div class="pricing-price text-primary">
                                <span class="pricing-currency">$</span>
                                <span class="pricing-main" data-bind-radio="pricing" data-monthly="29" data-yearly="39">29</span>
                                <span class="pricing-period text-primary">/ Week</span>
                            </div>
                        </div>
