                                    <i class="far fa-question-circle" data-toggle="tooltip" data-placement="top" title="To assist you in the ordering process, we've compiled a list of purchase and licensing related FAQs."></i>
                                </li>
                                <li class="m-list--link">
                                    - 10 campaigns/month <i class="far fa-question-circle" data-toggle="tooltip" data-placement="top" title="To assist you in the ordering process, we've compiled a list of purchase and licensing related FAQs."></i>
                                </li>
                                <li class="m-list--link">
                                    - Mirror website
                                    <span class="m-link-muted" data-toggle="tooltip" data-placement="top" title="To assist you in the ordering process, we've compiled a list of purchase and licensing related FAQs.">
                                        adjustmant
                                    </span>
                                </li>
                            </ul>
                            <button class="btn btn-outline-primary btn-block btn-sm border-w-2">Request Package</button>
                        </div>
                    </div>
                    <div class="pricing-item border-bottom border-top border-right">
                        <div class="pr-6 pl-6 pb-4 pt-5 bg-light-1 border-bottom">
                            <h3 class="pricing-title">Monthly Package</h3>
                            <div class="pricing-price text-warning">
                                <span class="pricing-currency">$</span>
                                <span class="pricing-main" data-bind-radio="pricing" data-monthly="79" data-yearly="99">79</span>
                                <span class="pricing-period text-warning">/ Month</span>
                            </div>
                        </div>
                        <div class="p-6">
                            <p class="pricing-sentence">For large companies</p>
                            <ul class="pricing-feature-list">
                                <li class="m-list--link">- Unlimited presentations</li>
                                <li class="m-list--link">- 5 hours of <span class="m-link-muted">free support</span> </li>
                                <li class="m-list--link">- Unlimited support</li>
                                <li class="m-list--link">- Unlimited <span class="m-link-muted">campaigns</span></li>
                            </ul>
                            <button class="btn btn-outline-warning btn-block btn-sm border-w-2">Request Package</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="section bg-light">
    <div class="container">
        <div class="w-md-80 w-lg-80 text-center mx-md-auto mb-9">
            <p class="text-primary">OUR BLOG</p>
            <h2> Our Company News</h2>
        </div>

        <div class="row justify-content-center">
            <div class="col-md-11">
                @(Html.Vertex().BlogPosts(BlogPostViewName.Card)
                                                    .ToSlider(slider =>
                                                    {
                                                        slider.SlidesToShow(3);
                                                        slider.Dots();
                                                    }).DisplayCount(6))
            </div>
            <div class="col-md-12 text-center mt-5">
                <p>Interested in more ? <a class="text-primary border-bottom anim-link" href="#">Keep Reading</a> </p>
            </div>
        </div>
    </div>
</section>


@Html.Vertex().Footer().ViewName("FooterDark")